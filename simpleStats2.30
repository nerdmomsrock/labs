/*given 4 integers, output their product and average using integer arithmetic */

import java.util.Scanner;

public class LabProgram {
    public static void main(String{} args) {
Scanner scnr = new Scanner(System.in);

int num1;
int num2;
int num3;
int num4;

num1 = scnr.nextInt();
num2 = scnr.nextInt();
num3 = scnr.nextInt();
num4 = scnr.nextInt();

int prod = num1 * num2 * num3 * num4;

int ave = (num1 + num2 + num3 + num4)/4;

System.out.println(prod + " " + ave);

double prod2 = (double)num1 * (double)num2 * (double)num3 * (double)num4;
double ave2 = ((double)num1 + (double)num2 + (double)num3 + (double)num4)/4;


/* also print product and average with floating point values three digits after decimal */
System.out.printf("%.3f", prod2);
System.out.print(" ");
System.out.printf("%.3f", ave2);
System.out.println();


    }
}